datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model ContentSubmission {
  id             Int @id @default(autoincrement())
  userId         String
  entryId        String
  sentenceCount  Int
  createdAt      DateTime @default(now())
}
